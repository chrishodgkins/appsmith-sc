  "searchKey": "",
  "tableData": "{{\n  gen1DiagramMap.data\n    .filter(row => row.account_no === Select1.selectedOptionValue)\n    .flatMap(row => {\n      if (!row.local_policy_summary) return [];\n      \n      const parseJsonArray = (str) => {\n        if (!str) return '';\n        try {\n          if (str.trim().startsWith('[')) {\n            const parsed = JSON.parse(str);\n            return Array.isArray(parsed) ? parsed.join(', ') : str;\n          }\n        } catch (e) {}\n        return str;\n      };\n      \n      return row.local_policy_summary.split(/\\|\\n/)\n        .filter(line => line.trim())\n        .map(line => {\n          const parts = line.split('<|>');\n          return {\n            data_source: parts[0] || '',\n            lp_from_range: parts[1] || '',\n            lp_to_range: parts[2] || '',\n            active: parts[3] || '',\n            src_realm: parseJsonArray(parts[4]),\n            hoplist: parts[5] || '',\n            dest_realms: parseJsonArray(parts[6])\n          };\n        });\n    })\n}}",
