break-word;\">\n        {{  \n          pullCustDB.data\n            .find(row => row.Account === appsmith.store.statclickaccount)\n            .numbers\n            .replaceAll('|', ' - ')\n        }}\n      </td>\n    </tr>\n    \n    <!-- GEN1 Section -->\n    <tr>\n      <td colspan=\"2\" style=\"padding: 4px 8px; background-color: #f5f5f5; border-bottom: 2px solid #333; border-top: 2px solid #333;\">\n        <strong>GEN1</strong>\n      </td>\n    </tr>\n    \n    <tr>\n      <td style=\"padding: 4px 8px; font-weight: bold; border-bottom: 1px solid #ddd; word-wrap: break-word;\">Account Status</td>\n      <td style=\"padding: 4px 8px; border-bottom: 1px solid #ddd; word-wrap: break-word;\">\n        <span style=\"color: {{ \n          (!Table1.tableData || Table1.tableData.length === 0) ? 'red' : \n          (Table1.tableData.every(row => row.active !== 'enabled')) ? 'red' :\n          (Table1.tableData.some(row => row.active !== 'enabled')) ? '#FF8C00' : \n          'green' \n        }}\">\n          {{ \n            (!Table1.tableData || Table1.tableData.length === 0) ? 'No Account Found' : \n            (Table1.tableData.every(row => row.active !== 'enabled')) ? 'Disabled Account' :\n            (Table1.tableData.some(row => row.active !== 'enabled')) ? 'Partially Disabled Policy' :\n            'Enabled'\n          }}\n        </span>\n      </td>\n    </tr>\n    \n    <tr>\n      <td style=\"padding: 4px 8px; font-weight: bold; border-bottom: 1px solid #ddd; word-wrap: break-word;\">Sites</td>\n      <td style=\"padding: 4px 8px; border-bottom: 1px solid #ddd; word-wrap: break-word; overflow-wrap: break-word;\">\n        {{ getNtwkData.data.find(row => row.account_no === appsmith.store.statclickaccount).Site1 }}, \n        {{ getNtwkData.data.find(row => row.account_no === appsmith.store.statclickaccount).Site2 }}\n      </td>\n    </tr>\n    \n    <!-- IVP Section -->\n    <tr>\n      <td colspan=\"2\" style=\"padding: 4px 8px; background-color: #f5f5f5; border-bottom: 2px solid #333; border-top: 2px solid #333;\">\n        <strong>IVP</strong>\n      </td>\n    </tr>\n    \n    <tr>\n      <td style=\"padding: 4px 8px; font-weight: bold; border-bottom: 1px solid #ddd; word-wrap: break-word;\">Account Status</td>\n      <td style=\"padding: 4px 8px; border-bottom: 1px solid #ddd; word-wrap: break-word;\">\n        <span style=\"color: {{ (manipTable.tableData.find(row => row.ivp_externalid === updateExIDCopy.text)?.ivp_accountstatus === 'Active') ? 'green' : 'red' }}\">\n          {{ manipTable.tableData.find(row => row.ivp_externalid === updateExIDCopy.text)?.ivp_accountstatus || 'No Account Found' }}\n        </span>\n      </td>\n    </tr>\n    \n    <tr>\n      <td style=\"padding: 4px 8px; font-weight: bold; border-bottom: 1px solid #ddd; word-wrap: break-word;\">IVP Service ID</td>\n      <td style=\"padding: 4px 8px; border-bottom: 1px solid #ddd; word-wrap: break-word; overflow-wrap: break-word;\">\n        {{ pullCustDB.data.find(row => row.Account === appsmith.store.statclickaccount).ivp_externalid }}\n      </td>\n    </tr>\n    \n    <tr>\n      <td style=\"padding: 4px 8px; font-weight: bold; vertical-align: top; border-bottom: 1px solid #ddd; word-wrap: break-word;\">ACL List (All Services)</td>\n      <td style=\"padding: 4px 8px; border-bottom: 1px solid #ddd; word-wrap: break-word; overflow-wrap: break-word;\">\n        {{ manipTable.tableData.find(row => row.ivp_externalid === updateExIDCopy.text).ACL }}\n      </td>\n    </tr>\n    \n    <tr>\n      <td style=\"padding: 4px 8px; font-weight: bold; border-bottom: 1px solid #ddd; word-wrap: break-word;\">Default CLI</td>\n      <td style=\"padding: 4px 8px; border-bottom",
